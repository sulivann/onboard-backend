{
  "connection": "default",
  "collectionName": "project",
  "info": {
    "name": "project",
    "description": "Project collection"
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "client": {
      "type": "string",
      "required": true
    },
    "senior": {
      "model": "user",
      "via": "projectAsSenior",
      "plugin": "users-permissions"
    }
  }
}