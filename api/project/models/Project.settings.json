{
  "connection": "default",
  "collectionName": "project",
  "info": {
    "name": "project",
    "description": "Project collection"
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "categories": {
      "collection": "category",
      "via": "projects",
      "dominant": true
    },
    "estimatedDays": {
      "type": "integer",
      "required": true
    },
    "clientName": {
      "type": "string",
      "required": true
    },
    "clientLogo": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "clientContactName": {
      "type": "string",
      "required": true
    },
    "clientContactEmail": {
      "type": "email",
      "required": true
    },
    "clientContactNumber": {
      "type": "string",
      "required": true
    },
    "senior": {
      "model": "user",
      "via": "projectsAsSenior",
      "plugin": "users-permissions"
    },
    "approved": {
      "type": "boolean"
    },
    "junior": {
      "model": "user",
      "via": "projectsAsJunior",
      "plugin": "users-permissions"
    },
    "objectives": {
      "collection": "objective",
      "via": "relatedProject"
    },
    "applications": {
      "collection": "application",
      "via": "project"
    },
    "price": {
      "type": "float",
      "required": true
    },
    "isDone": {
      "type": "boolean"
    }
  }
}