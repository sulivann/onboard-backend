{
  "connection": "default",
  "info": {
    "name": "user",
    "description": ""
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "projectsAsSenior": {
      "collection": "project",
      "via": "senior"
    },
    "projectsAsCandidate": {
      "collection": "project",
      "via": "candidates"
    },
    "projectsAsJunior": {
      "model": "project",
      "via": "junior"
    },
    "picture": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "firstname": {
      "type": "string"
    },
    "lastname": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "phone": {
      "type": "string"
    },
    "job": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "linkedinUrl": {
      "type": "string"
    },
    "siret": {
      "type": "string"
    },
    "rib": {
      "type": "string"
    }
  },
  "collectionName": "users-permissions_user"
}